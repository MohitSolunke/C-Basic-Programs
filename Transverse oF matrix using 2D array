#include<iostream>
using namespace std;


int main()
{
    int n;
    cout<<"Enter the Size of 2D matrix =";
    cin>>n;
    
    int A[n][n];
    cout<<"Enter the Inputs For Matrix \n";
    for(int i=0;i<3;i++)
    {
        for(int j=0;j<3;j++)
        {
            cin>>A[i][j];
        }
    }
    
    cout<<"Your Matrix is ğŸ‘‡ \n";
    
    for(int i=0;i<3;i++)
    {
        for(int j=0;j<3;j++)
        {
            cout<<A[i][j] <<" ";
        }
        cout<<"\n";
    }
    
    cout<<"Transverse of Above Matrix ğŸ‘‡ : \n";
    
    for(int i=0;i<3;i++)
    {
        for(int j=i;j<3;j++)
        {
            //swap
            int temp=A[i][j];
            A[i][j]=A[j][i];
            A[j][i]=temp;
        }
    
    }
    //print
    
    for(int i=0;i<3;i++)
    {
        for(int j=0;j<3;j++)
        {
            cout<<A[i][j]<<" ";
        }
        cout<<"\n";
    }
    
    return 0;
    
}
