#include<iostream>
using namespace std;

int divisible(int n,int a,int b)
{
    int c1=n/a;
    int c2=n/b;
    int c3=n/(a*b);
    
    return(c1+c2-c3);
    
}
int main()
{
    int n,a,b;
    cout<<"Enter the input for last no : ";
    cin>>n;
    cout<<"Enter the value for a and b : ";
    cin>>a>>b;
    cout<<"Total no of way to divisible "<<n<<" by "<<a<<"  and"<<b<<" is:";
    cout<<divisible(n,a,b)<<endl;
    return 0;
}
