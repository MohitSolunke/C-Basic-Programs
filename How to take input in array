#include<iostream>
using namespace std;

int main()
{
    int n;
    cout<<"Enter the size of an array : ";
    cin>>n;
    
    int arr[n];
    cout<<"enter the element for an array :"<<endl;
    for(int i=0;i<n;i++)
    {
        cin>>arr[i];
        
    }
    
    cout<<"Your array is......."<<endl;
    for(int i=0;i<n;i++)
    {
        cout<<arr[i]<<" ";
    }
    return 0;
}
